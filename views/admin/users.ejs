<!DOCTYPE html>
<html>
    <% include ../partials/head %>
    <body>
        <% include ../partials/admin_nav %>

        <h3>Users</h3>
        <div class="flash-msg"><%=message%></div>
        <table class="table table-striped">
            <thead>
                <% for(var attr in user_attributes){ %>
                <th>
                    <%= attr %>
                    <%if(user_attributes[attr]._autoGenerated){%>
                    (auto)
                    <%}%>
                </th>
                <% } %>
            </thead>
            <% for(var i=0; i<users.length; i++) {%>
            <tr>
                <% for(var key in users[i]){ %>
                    <td><%= users[i][key] %></td>
                <% } %>
            </tr>
            <% } %>
        </table>
        <% include ../partials/script_footer.ejs %>
    </body>
</html>
